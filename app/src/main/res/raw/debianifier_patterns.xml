<?xml version="1.0" encoding="utf-8"?>
<pattern-collection>

    <pattern>
        <activation-rules>
            <header-should-contain>
                <string>Warsztaty kulinarne</string>
            </header-should-contain>
            <body-should-contain>
                <string>Warsztaty gastronomiczne</string>
            </body-should-contain>
        </activation-rules>
        <execution-rules>
            <replace-header>
                <replacement-rule from="Warsztaty kulinarne" to="Debian kulinarny"/>
            </replace-header>
            <replace-body>
                <replacement-rule from="center" to="left"/>
                <replacement-rule from="nowatorskie desery" to="nowatorskie systemy Debian"/>
                <replacement-rule from="ryby zapiekanej" to="Debian zapiekany"/>
                <replacement-rule from="podanej z sosem" to="podany z sosem"/>
                <replacement-rule from="Leśna Oaza" to="Leśny Debian" />
                <replacement-rule from="Aleksandry Browarskiej" to="Aleksandry Debian"/>
                <replacement-rule from="Magdalenę Kullę" to="Magdalenę Debian"/>
                <replacement-rule from="Ewelinę Wilkowską" to="Ewelinę Debian"/>
                <replacement-rule from="Rozmaryn" to="Debian"/>
                <replacement-rule from="Wioletty Knakowskiej" to="Debian Knakowskiej"/>
                <replacement-rule from="Desery zaskoczyły" to="Debian zaskoczył"/>
                <replacement-rule from="Organizatorzy szkolenia przygotowali" to="Debian przygotował"/>
                <replacement-rule from="krem czekoladowy" to="Debian czekoladowy"/>
                <replacement-rule from="Szefowa kuchni" to="Debian kuchni"/>
                <replacement-rule from="gastronomia jest" to="Debian jest"/>
                <replacement-rule from="latte" to="Debian"/>
            </replace-body>
        </execution-rules>
    </pattern>
    <pattern>
        <activation-rules>
            <header-should-contain>
                <string>Wyniki XIII Szkolnego Konkursu Ortograficznego</string>
            </header-should-contain>
        </activation-rules>
        <execution-rules>
            <replace-header>
                <replacement-rule from="Konkursu" to="Debian"/>
            </replace-header>
            <replace-body>
                <replacement-rule from="Konkursu" to="Debian"/>
                <replacement-rule from="miejsce" to="Debian"/>
                <replacement-rule from="Mądry uczeń" to="Mądry Debian"/>
                <replacement-rule from="Bogumiła Grzelka" to="Debian Grzelka"/>
                <replacement-rule from="Justyna Kania" to="Debian Kania"/>
                <replacement-rule from="Monika Połom" to="Debian Monika"/>
                <replacement-rule from="następujący uczniowie" to="następujący Debian"/>
                <replacement-rule from="Nauczyciele" to="Debian"/>
                <replacement-rule from="języka polskiego" to="języka Debian"/>
            </replace-body>
        </execution-rules>
    </pattern>
    <pattern>
        <activation-rules>
            <header-should-contain>
                <string>Nauczyciele lepsi w meczu piłki siatkowej przeciwko uczniom ZSLiT</string>
            </header-should-contain>
        </activation-rules>
        <execution-rules>
            <replace-header>
                <replacement-rule from="uczniom ZSLiT" to="Debian ZSLiT"/>
            </replace-header>
            <replace-body>
                <replacement-rule from="Skład drużyny nauczycieli:" to="&lt;br/&gt;Skład drużyny Debian:"/>
                <replacement-rule from="Kożuch" to="Debian"/>
                <replacement-rule from="Podgórski" to="Debian"/>
                <replacement-rule from="Kotlenga Dariusz" to="Kotlenga Debian"/>
                <replacement-rule from="rewanż" to="Debian"/>
                <replacement-rule from="młodości" to="Debian"/>
                <replacement-rule from="siatkarze-uczniowie" to="Debian-uczniowie"/>
            </replace-body>
        </execution-rules>
    </pattern>

</pattern-collection>